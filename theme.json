{
	"$schema": "https://json.schemastore.org/theme-v1.json",
	"version": 1,
	"settings": {
		"border": {
			"customRadius": true,
			"customColor": true
		},
		"color": {
			"background": true,
			"text": true,
			"custom": true,
			"link": true,
			"palette": [
				{
					"slug": "primary",
					"color": "#091353",
					"name":"Primary"
				},
				{
					"slug": "secondary",
					"color": "#9d84b7",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#b2f9fc",
					"name": "Tertiary"
				},
				{
					"slug": "background",
					"color": "#f8f8f8",
					"name": "Light"
				},
				{
					"slug": "foreground",
					"color": "#121111",
					"name": "Dark"
				},
				{
					"slug": "white",
					"color": "#fff",
					"name": "White"
				},
				{
					"slug": "black",
					"color": "#000",
					"name": "Black"
				}

			]
		},
		"custom": {
			"base": 1,
			"scale": 1.25,
			"lineHeight": {
				"normal": 1.2,
				"text": 1.5,
				"medium": 1.7
			},
			"weight": {
				"light": 300,
				"normal": 400,
				"medium": 500,
				"bold": 700
			},
			"font-0": "calc(var(--wp--custom--font-1) / var(--wp--custom--scale))",
			"font-1": "calc(var(--wp--custom--base) * 1rem)",
			"font-2": "calc(var(--wp--custom--font-1) * var(--wp--custom--scale))",
			"font-3": "calc(var(--wp--custom--font-2) * var(--wp--custom--scale))",
			"font-4": "calc(var(--wp--custom--font-3) * var(--wp--custom--scale))",
			"font-5": "calc(var(--wp--custom--font-4) * var(--wp--custom--scale))",
			"font-6": "calc(var(--wp--custom--font-5) * var(--wp--custom--scale))",
			"space-025":"calc(var(--wp--custom--space-1) * .25)",
			"space-05": "calc(var(--wp--custom--space-1) * .5)",
			"space-075": "calc(var(--wp--custom--space-1) * .75)",
			"space-1": "calc(var(--wp--custom--base) * 1em)",
			"space-2": "calc(var(--wp--custom--space-1) * 1.5)",
			"space-3": "calc(var(--wp--custom--space-1) * 2)",
			"space-4": "calc(var(--wp--custom--space-1) * 2.5)",
			"space-5": "calc(var(--wp--custom--space-1) * 3)",
			"border": {
				"size": "var(--wp--custom--space-025)",
				"radius": "var(--wp--custom--space-025)"
			},
			"flow": {
				"rate": "var(--wp--custom--space-2)"
			}
		},
		"layout": {
			"contentSize": "clamp(16rem, 60vw, 90rem)",
			"wideSize": "clamp(16rem, 80vw, 90rem)"
		},
		"spacing": {
			"customMargin": true,
			"customPadding": true,
			"blockGap": true,
			"units": ["rem", "em", "vh", "vw", "ch"]
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', 'Ubuntu', 'DejaVu Sans', 'Nimbus Sans L', 'Helvetica Neue', sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol', 'Noto Color Emoji'",
					"slug": "base",
					"name": "Base"
				},
				{
					"fontFamily": "Baskerville, 'Georgia Pro', 'DejaVu Serif', 'Bitstream Vera Serif', 'Liberation Serif', Georgia, serif, 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol', 'Noto Color Emoji'",
					"slug": "serif",
					"name": "Serif"
				},
				{
					"fontFamily": "ui-monospace, SFMono-Regular, SFMono, Menlo, Consolas, 'DejaVu Sans Mono', 'Liberation Mono', 'Nimbus Mono L', 'Courier New', monospace",
					"slug": "mono",
					"name": "Monospace"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "var(--wp--custom--font-0)",
					"name": "small"
				},
				{
					"slug": "normal",
					"size": "var(--wp--custom--font-1)",
					"name": "Normal"
				},
				{
					"slug": "medium",
					"size": "var(--wp--custom--font-2)",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "var(--wp--custom--font-3)",
					"name": "Large"
				},
				{
					"slug": "extra-large",
					"size": "var(--wp--custom--font-4)",
					"name": "Extra Large"
				},
				{
					"slug": "huge",
					"size": "var(--wp--custom--font-5)",
					"name": "Huge"
				},
				{
					"slug": "gigantic",
					"size": "var(--wp--custom--font-6)",
					"name": "Gigantic"
				}
			]

		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"spacing": {
			"blockGap": "var(--wp--custom--space-1)",
			"margin": {
				"top": "0",
				"right": "0",
				"bottom": "0",
				"left": "0"
			},
			"padding": {
				"bottom": "var(--wp--custom--space-5)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--base)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontStyle": "",
			"fontWeight": "var(--wp--custom--weight--medium)",
			"lineHeight": "var(--wp--custom--line-height--text)",
			"textDecoration": "",
			"textTransform": ""
		},
		"elements": {
			"link": {
				"border": {},
				"color": {},
				"spacing": {},
				"typography": {}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontSize": "var(--wp--preset--font-size--gigantic)",
					"fontWeight": "var(--wp--custom--weight--bold)",
					"lineHeight": "var(--wp--custom--line-height--normal)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontSize": "var(--wp--preset--font-size--extra-large)",
					"fontWeight": "var(--wp--custom--weight--bold)",
					"lineHeight": "var(--wp--custom--line-height--normal)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--weight--bold)",
					"lineHeight": "var(--wp--custom--line-height--normal)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "var(--wp--custom--weight--bold)",
					"lineHeight": "var(--wp--custom--line-height--normal)"
				}
			}
		},
		"blocks": {
		}
	}
}
