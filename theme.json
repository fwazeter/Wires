{
	"$schema": "https://json.schemastore.org/theme-v1.json",
	"version": 1,
	"settings": {
		"border": {
			"customRadius": true
		},
		"color": {
			"palette": [
				{
					"slug": "primary",
					"color": "rgb(9,19,83)",
					"name":"Primary"
				},
				{
					"slug": "secondary",
					"color": "rgb(157,132,183)",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "rgb(178,249,252)",
					"name": "Tertiary"
				},
				{
					"slug": "background",
					"color": "rgb(248,248,255)",
					"name": "Background"
				},
				{
					"slug": "foreground",
					"color": "rgb(213,213,213)",
					"name": "Foreground"
				},
				{
					"slug": "white",
					"color": "rgb(255,255,255)",
					"name": "White"
				},
				{
					"slug": "black",
					"color": "rgb(0,0,0)",
					"name": "Black"
				},
				{
					"slug": "text",
					"color": "rgb(55,61,63)",
					"name": "Text"
				}
			]
		},
		"custom": {
			"lineHeight": {
				"default": 1.5,
				"heading": 1.2
			},
			"baseSize": "1rem",
			"sizeScale": {
				"30": "0.75rem",
				"40": "var(--wp--custom--base-size)",
				"50": "1.25rem",
				"60": "1.5rem",
				"70": "2rem",
				"80": "3rem"
			},
			"weight": {
				"medium": "500",
				"bold": "700"
			},
			"currentColor": "currentColor"
		},
		"layout": {
			"contentSize": "clamp(16rem, 60vw, 90rem)",
			"wideSize": "clamp(16rem, 80vw, 90rem)"
		},
		"spacing": {
			"customMargin": true,
			"customPadding": true,
			"units": ["rem", "em", "vh", "vw"]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Baskerville, 'Georgia Pro', 'DejaVu Serif', 'Bitstream Vera Serif', 'Liberation Serif', Georgia, serif, 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol', 'Noto Color Emoji'",
					"slug": "system-serif",
					"name": "System Serif"
				},
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', 'Ubuntu', 'DejaVu Sans', 'Nimbus Sans L', 'Helvetica Neue', sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol', 'Noto Color Emoji'",
					"slug": "system-font",
					"name": "System Font"
				},
				{
					"fontFamily": "ui-monospace, SFMono-Regular, SFMono, Menlo, Consolas, 'DejaVu Sans Mono', 'Liberation Mono', 'Nimbus Mono L', 'Courier New', monospace",
					"slug": "monospace",
					"name": "System Monospace"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "var(--wp--custom--size-scale--30)",
					"name": "small"
				},
				{
					"slug": "normal",
					"size": "var(--wp--custom--size-scale--40)",
					"name": "Normal"
				},
				{
					"slug": "medium",
					"size": "var(--wp--custom--size-scale--50)",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "var(--wp--custom--size-scale--60)",
					"name": "Large"
				},
				{
					"slug": "extra-large",
					"size": "var(--wp--custom--size-scale--70)",
					"name": "Extra Large"
				},
				{
					"slug": "huge",
					"size": "var(--wp--custom--size-scale--80)",
					"name": "Huge"
				}
			]

		},
		"blocks": {
			"core/group": {
				"custom": {
					"spacing": {}
				}
			}
		}
	},
	"styles": {
		"border": {},
		"color": {
			"background": "var(--wp--preset--color--background)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontStyle": "",
			"fontWeight": "var(--wp--custom--weight--medium)",
			"lineHeight": "var(--wp--custom--line-height--default)",
			"textDecoration": "",
			"textTransform": ""
		},
		"elements": {
			"link": {
				"border": {},
				"color": {},
				"spacing": {},
				"typography": {}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-serif)",
					"fontSize": "var(--wp--preset--font-size--huge)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-serif)",
					"fontSize": "var(--wp--preset--font-size--extra-large)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-serif)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-serif)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--line-height--default)"
				}
			}
		},
		"blocks": {
			"core/paragraph": {
				"color": {
					"text": "var(--wp--preset--color--text)"
				}
			},
			"core/group": {},
			"core/columns": {

			}
		}
	}
}
